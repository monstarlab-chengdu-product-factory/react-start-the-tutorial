{
  "name": "ts-cms",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.5.1",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-scripts": "1.1.4",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/assets/stylesheet -o src/assets/stylesheet/css",
    "watch-css": "npm run build-css && node-sass-chokidar src/assets/stylesheet -o src/assets/stylesheet/css --watch --recursive",
    "start-js": "react-app-rewired start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-app-rewired build",
    "build": "npm-run-all build-css build-js",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "serve": "npm run build & serve build"
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "babel-plugin-import": "^1.7.0",
    "humps": "^2.0.1",
    "lodash": "^4.17.5",
    "prop-types": "^15.6.1",
    "react-app-rewired": "^1.5.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0"
  }
}
